{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile.frontend",
    "watchPatterns": [
      "frontend/**",
      "railway.json",
      "Dockerfile.frontend",
      ".github/**"
    ]
  },
  "deploy": {
    "healthcheckPath": "/",
    "healthcheckTimeout": 100,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 3
  }
  // Force redeploy - Railway should now detect all changes
}
